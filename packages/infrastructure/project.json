{
  "root": "packages/infrastructure",
  "sourceRoot": "packages/infrastructure",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/node:package",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/infrastructure",
        "tsConfig": "packages/infrastructure/tsconfig.lib.json",
        "packageJson": "packages/infrastructure/package.json",
        "main": "packages/infrastructure/index.ts",
        "assets": ["packages/infrastructure/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/infrastructure/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/infrastructure"],
      "options": {
        "jestConfig": "packages/infrastructure/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
